            <div class="container" style="border-bottom:1px solid black;width:100%;height:10px; margin:3rem 0rem;"></div>
        </div>



        <div class="container">
            <div class="table-responsive">
                <table class="table table-striped">
                    <tr>
                        <td>
                            <p id="sex"><%= __('sex.desc') %></p>
                        </td>
                        <td onclick="checkGender('male')">
                            <input type="radio" align="center" id="male" name="gender" value="male" />
                            <%= __('sex.male') %>
                        </td>
                        <td onclick="checkGender('female')">
                            <input type="radio" align="center" id="female" name="gender" value="female" />
                            <%= __('sex.female') %>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <p id="descAge">
                            <%= __('age.desc') %></p>
                        </td>
                        <td>
                            <input type="number" name="userAge"id="age" />
